<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <data></data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.aries.ui.view.title.TitleBarView
            android:id="@+id/title_bar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/title_bar_height"
            android:background="@color/white"
            android:visibility="visible"
            app:title_actionPadding="@dimen/title_bar_action_padding"
            app:title_actionTextColor="@color/black"
            app:title_actionTint="@color/black"
            app:title_outPadding="@dimen/title_bar_out_padding"
            app:title_statusBarLightMode="true"
            app:title_titleMainText="@string/app_name"
            app:title_titleMainTextColor="@color/black"
            app:title_titleMainTextFakeBold="true"
            app:title_titleMainTextSize="@dimen/dimen_15sp"
            />

        <FrameLayout
            android:id="@+id/fl_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"/>
    </LinearLayout>
</layout>
